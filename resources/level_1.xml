<level recipeName="recipeName">
	<!--Type of recipe i.e. cajun, Americana, French etc...-->
	<Type recipeType="recipeType"/>
	<!--Actual ingredients in recipe-->
	<ingredients
		ingredients="greenbeans"
		ingredients="salt"
		ingredients="pepper"
		ingredients="cranberries"
		ingredients="milk"
		ingredients="breadcrumbs"
		ingredients="butter"
	/>
	
	<!--Red herrings-->	
	<nonIngredients
		dummyIngredients="orange"
		dummyIngredients="turkey"
	/>
	
	<!--score needed to beat the level-->
	<score
		scoreToBeat="100"
	/>
</level>